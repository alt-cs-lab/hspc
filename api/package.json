{
  "name": "hspc-api",
  "version": "1.0.0",
  "description": "API",
  "main": "server.js",
  "scripts": {
    "start": "node ./src/start.js",
    "dev": "nodemon ./src/start.js",
    "debug": "node --inspect ./src/start.js",
    "build": "cd react && npm run build",
    "docs": "apidoc -i src/ -o apidoc/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:handles": "jest --detectOpenHandles"
  },
  "author": "Daniel Bell, Joe Webster, Kyle Fairfax, Josh Weese",
  "license": "MIT",
  "dependencies": {
    "apidocs": "^2017.3.9",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.2.0",
    "fast-csv": "^4.3.6",
    "fast-xml-parser": "^4.1.1",
    "helmet": "^3.21.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mssql": "^4.3.7",
    "multer": "^1.4.2",
    "nodejs": "0.0.0",
    "nodemon": "^2.0.13",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg-promise": "^10.15.4",
    "postgresql-client": "^2.4.1",
    "react": "^16.11.0",
    "reactjs-popup": "^1.5.0",
    "socket.io": "^2.3.0",
    "validator": "^13.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      "db",
      "client",
      "/node_modules/",
      "/__tests__/fixtures",
      "react/build"
    ],
    "coveragePathIgnorePatterns": [
      "db",
      "client",
      "/node_modules/",
      "/__tests__/fixtures",
      "react/build"
    ],
    "modulePathIgnorePatterns": [
      "db",
      "client",
      "/node_modules/",
      "/__tests__/fixtures",
      "react/build"
    ]
  },
  "devDependencies": {
    "apidoc": "^0.53.1",
    "child_process": "^1.0.2",
    "child-process": "^1.0.2",
    "jest": "^29.3.1",
    "supertest": "^6.3.3"
  }
}
